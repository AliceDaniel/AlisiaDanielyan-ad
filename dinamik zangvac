#include <iostream>
using namespace std;
int main()
{
   unsigned row,column;
   cin>>row>>column;
   int arr1[row][column];
   int** arr=new int*[row];
   for(int i =0;i<row;i++){
       arr[i]=new int[column];
       for(int j=0;j<column;j++){
           cin>>arr[i][j];
       }
   } for(int i =0;i<row;i++){
       for(int j=0;j<column;j++){
           arr1[i][j]=arr[j][i];
       }
   } for(int i =0;i<row;i++){
       for(int j=0;j<column;j++){
           cout<<arr1[i][j]<<" ";
       }
       cout<<endl;
   }
   
    return 0;
}
\\\\\\\\\\glxavor diaganali elemi gumar\\\\
#include <iostream>
using namespace std;
int main()
{
   unsigned row,column,sum=0;
   cin>>row>>column;
   int** arr=new int*[row];
   for(int i =0;i<row;i++){
       arr[i]=new int[column];
       for(int j=0;j<column;j++){
           cin>>arr[i][j];
       }
   }for(int i =0;i<row;i++){
       for(int j=0;j<column;j++){
           cout<<arr[i][j]<<",";
       }
       cout<<endl;
   }
   for(int i=0;i<row && i<column;i++){
       sum+=arr[i][i];
   }
   cout<<endl<<"sum="<<sum;
    return 0;
}
